{
  "name": "modern-jquery-project",
  "version": "1.0.0",
  "description": "jquery website tempalte build with gulp and webpack.",
  "main": "none",
  "scripts": {
    "dev": "gulp --gulpfile config/gulpfile.js --cwd ./",
